<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:avaloniaConverters="clr-namespace:Avalonia.Controls.Converters;assembly=Avalonia.Controls"
					xmlns:conv="using:LibreDiagnostics.UI.Converter">
    <ResourceDictionary.MergedDictionaries>
		<ResourceInclude Source="avares://BlackSharp.UI.Avalonia/Converter/Converter.axaml"/>
    </ResourceDictionary.MergedDictionaries>

    <avaloniaConverters:EnumToBoolConverter x:Key="EnumToBoolConverter"/>

    <conv:AlertColorConverter              x:Key="AlertColorConverter"/>
    <conv:FontToSpaceConverter             x:Key="FontToSpaceConverter"/>
    <conv:MetricConfigToSelectionConverter x:Key="MetricConfigToSelectionConverter"/>
    <conv:StringToImageConverter           x:Key="StringToImageConverter"/>

</ResourceDictionary>
